<%= form_with model: kitten, local: true do |f| %>
  <%= f.label :name, 'Name:' %>
  <%= f.text_field :name %>

  <%= f.label :age, 'Age:' %>
  <%= f.select :age, (1..25) %>

  <%= f.label :cuteness, 'Cuteness Level:' %>
  <%= f.select :cuteness, (1..10) %>

  <%= f.label :softness, 'Softness Level:' %>
  <%= f.select :softness, (1..10) %><br />

  <%= f.submit %>
<% end %>